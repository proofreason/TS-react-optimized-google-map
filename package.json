{
  "name": "@proofreason/ts-react-optimized-google-map",
  "version": "0.1.4",
  "description": "Typescript google maps wrapper optimized for react applications",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "dev": "webpack-dev-server --open --config webpack/webpack.dev.js",
    "prod": "webpack --open --config webpack/webpack.prod.js",
    "clean": "rm -rf ./lib & rm -rf ./lib-esm & rm -rf ./dist",
    "build": "npm run clean & ttsc -p ./tsconfig.prod.json & ttsc -p ./tsconfig.prod.json -m es6 --outDir lib-esm",
    "build-all": "npm run build & npm run prod",
    "update:packages": "node wipe-dependencies.js && rm -rf node_modules && npm update --save-dev && npm update --save",
    "tslint-fix": "tslint --fix -c ./tslint.json '{src,develop}/**/*{.ts,.tsx}'",
    "test": "jest --no-cache --config __jest__/jest.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git@git.proofreason.com:e.on/eon-drupal.git"
  },
  "lint-staged": {
    "{src,develop}/**/*.{ts,tsx}": [
      "tslint -c ./tslint.json"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "author": "Erik Zatloukal <zatloukal.erik@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/react": "^10.4.3",
    "@types/jest": "^26.0.3",
    "@types/node": "^14.0.14",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@types/rewire": "^2.5.28",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "awesome-typescript-loader": "^5.2.1",
    "babel-jest": "^26.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "dotenv-webpack": "^1.8.0",
    "file-loader": "^6.0.0",
    "filemanager-webpack-plugin": "^2.0.5",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "path": "^0.12.7",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-test-renderer": "^16.13.1",
    "require-dir": "^1.2.0",
    "rewire": "^5.0.0",
    "speed-measure-webpack-plugin": "^1.3.3",
    "terser-webpack-plugin": "^3.0.6",
    "ts-jest": "^26.1.1",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-loader": "^3.5.4",
    "tslint-plugin-prettier": "^2.3.0",
    "tslint-react": "^5.0.0",
    "ttypescript": "^1.5.10",
    "typescript": "^3.9.6",
    "utility-types": "^3.10.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  },
  "peerDependencies": {
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "dependencies": {
    "@types/googlemaps": "^3.39.8",
    "@types/markerclustererplus": "^2.1.33",
    "google-maps-infobox": "^2.0.0",
    "marker-clusterer-plus": "^2.1.4",
    "ts-overlapping-marker-spiderfier": "^1.0.3"
  },
  "keywords": [
    "react",
    "google",
    "maps"
  ],
  "files": ["lib/","lib-esm/", "dist", "index.d.ts"],
  "directories": {
    "lib": "lib"
  }
}
